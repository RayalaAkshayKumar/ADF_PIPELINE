{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mydatasfactorys2025"
		},
		"Ls_sql25_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Ls_sql25'"
		},
		"ls_datalake25_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake25'"
		},
		"ls_mysqldatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_mysqldatabase'"
		},
		"ls_sqldatabase25_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sqldatabase25'"
		},
		"Ls_sql25_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mysqlsserver2025.database.windows.net"
		},
		"Ls_sql25_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldatabase25"
		},
		"Ls_sql25_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin2025"
		},
		"ls_datalake25_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mydatalaess25.dfs.core.windows.net/"
		},
		"ls_mysqldatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mysqlsserver2025.database.windows.net"
		},
		"ls_mysqldatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mydatabasepractice2025"
		},
		"ls_mysqldatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin2025"
		},
		"ls_sqldatabase25_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mysqlsserver2025.database.windows.net"
		},
		"ls_sqldatabase25_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldatabase25"
		},
		"ls_sqldatabase25_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin2025"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mydatasfactorys2025_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Ls_sql25_password": {
						"value": "[parameters('Ls_sql25_password')]"
					},
					"ls_datalake25_accountKey": {
						"value": "[parameters('ls_datalake25_accountKey')]"
					},
					"ls_mysqldatabase_password": {
						"value": "[parameters('ls_mysqldatabase_password')]"
					},
					"ls_sqldatabase25_password": {
						"value": "[parameters('ls_sqldatabase25_password')]"
					},
					"Ls_sql25_properties_typeProperties_server": {
						"value": "[parameters('Ls_sql25_properties_typeProperties_server')]"
					},
					"Ls_sql25_properties_typeProperties_database": {
						"value": "[parameters('Ls_sql25_properties_typeProperties_database')]"
					},
					"Ls_sql25_properties_typeProperties_userName": {
						"value": "[parameters('Ls_sql25_properties_typeProperties_userName')]"
					},
					"ls_datalake25_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake25_properties_typeProperties_url')]"
					},
					"ls_mysqldatabase_properties_typeProperties_server": {
						"value": "[parameters('ls_mysqldatabase_properties_typeProperties_server')]"
					},
					"ls_mysqldatabase_properties_typeProperties_database": {
						"value": "[parameters('ls_mysqldatabase_properties_typeProperties_database')]"
					},
					"ls_mysqldatabase_properties_typeProperties_userName": {
						"value": "[parameters('ls_mysqldatabase_properties_typeProperties_userName')]"
					},
					"ls_sqldatabase25_properties_typeProperties_server": {
						"value": "[parameters('ls_sqldatabase25_properties_typeProperties_server')]"
					},
					"ls_sqldatabase25_properties_typeProperties_database": {
						"value": "[parameters('ls_sqldatabase25_properties_typeProperties_database')]"
					},
					"ls_sqldatabase25_properties_typeProperties_userName": {
						"value": "[parameters('ls_sqldatabase25_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}